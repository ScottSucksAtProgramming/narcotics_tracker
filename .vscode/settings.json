        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "hide-files.files": [],
    "vsicons.associations.folders": [
        {
            "icon": "test",
            "extensions": [
                "unit",
